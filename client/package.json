{
  "name": "prop-exchange-ui",
  "description": "prop-exchange-ui",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.2",
    "@fortawesome/free-brands-svg-icons": "5.2.0",
    "@fortawesome/free-regular-svg-icons": "5.2.0",
    "@fortawesome/free-solid-svg-icons": "5.2.0",
    "@fortawesome/react-fontawesome": "0.1.0",
    "antd": "3.7.3",
    "connected-react-router": "4.3.0",
    "d3-scale": "2.1.2",
    "leaflet": "1.3.3",
    "leaflet-polylineoffset": "1.1.1",
    "leaflet-rotatedmarker": "0.2.0",
    "lodash": "4.17.10",
    "moment": "2.22.2",
    "node-uuid": "1.4.8",
    "npm-run-all": "4.1.3",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-leaflet": "2.0.0",
    "react-redux": "5.0.7",
    "react-redux-toastr": "7.4.0",
    "react-resizable": "1.7.5",
    "react-router-dom": "4.3.1",
    "react-scripts": "1.1.4",
    "react-sortable-hoc": "0.8.3",
    "react-vis": "1.11.3",
    "redux": "4.0.0",
    "redux-devtools-extension": "2.13.5",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "antd-scss-theme-plugin": "1.0.7",
    "babel-plugin-import": "1.2.0",
    "eslint-plugin-react-redux": "2.3.0",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "react-app-rewired": "1.2.9",
    "sass-loader": "6.0.6"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:react/recommended",
      "plugin:react-redux/recommended"
    ],
    "plugins": [
      "react",
      "react-redux"
    ],
    "rules": {
      "semi": [
        1,
        "always"
      ],
      "linebreak-style": [
        0,
        "unix"
      ],
      "jsx-quotes": [
        1,
        "prefer-single"
      ],
      "curly": "warn",
      "react/prop-types": [
        1,
        {
          "ignore": [
            "children"
          ]
        }
      ]
    }
  },
  "engines": {
    "node": "==10.3"
  }
}
